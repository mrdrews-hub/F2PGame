{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle" : "off",
        "import/no-extraneous-dependencies" : "off",
        "eqeqeq" : "off",
        "no-plusplus" : "off",
        "max-len" : "off"
    }
}
